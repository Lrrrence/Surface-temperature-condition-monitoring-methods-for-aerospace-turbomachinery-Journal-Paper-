@article{Carter2005,
abstract = {Modern aviation gas turbine engines are considered to be highly reliable in that failures in service are rare. In fact this is a misconception, and freedom from service failures is largely the result of stringent standards imposed during frequent inspections. Most failures are thus detected at the incipient stage and appropriate action taken to prevent service failure. The common failure mechanisms found in gas turbine blades are discussed and illustrated. {\textcopyright} 2004 Elsevier Ltd. All rights reserved.},
