issn = {24104833},
journal = {12th Eur. Conf. Turbomach. Fluid Dyn. Thermodyn. ETC 2017},
keywords = {Engine test,Gas turbine,Temperature measurement},
pages = {1--13},
title = {{Surface temperature measurements in an industrial gas turbine using thermal history paints}},
year = {2017}
}
