title = {{Thermal barrier coatings - A state of the art review}},
url = {https://doi.org/10.1007/s12540-020-00705-w},
volume = {1},
year = {2020}
}
@article{Duan2018,
abstract = {Robust embedded MEMS sensors capable of measuring high temperature in aero-engine combustion environment plays an important role in developing more efficient and intelligent aero-engine. In this paper, thermistor and thermal couple devices were in-situ fabricated directly on nozzle guide vane surface. High temperature characterization and various tough vibration-and shocking test were performed on these embedded sensors. The sensor system (including sensor and the wiring) is capable of measuring up to 1200°C high temperature and standing for up to 40g 10-2000Hz vibration and 100g shocking test. These embedded sensors also survived hot gas turbine test with pressure gas flow of 1200°C/ 0.5MPa and exhibits reliable temperature readings. This development is very useful to quantify the temperature on the sensitive spot of turbine components for aero engine R{\&}D purpose and for monitoring the temperature in combustion chamber and turbine areas. These thin film sensors are within one $\mu$m thick with line-width within a few hundred $\mu$m therefore bring about minimum turbulence to the environmental gas flows.},
author = {Duan, Franklin Li and Hu, Mingkai and Ding, Guifu and Li, Jibao and Lin, Yuzhen and Zou, Binglin},
doi = {10.2514/6.2018-4622},
file = {:C$\backslash$:/Users/yulel/OneDrive - University of Southampton/LYule PhD Study Shared/Gas turbine sensing/Thermocouples/HL-350727.pdf:pdf},
